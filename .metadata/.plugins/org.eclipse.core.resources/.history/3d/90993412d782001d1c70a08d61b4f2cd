import java.util.Scanner;

public class lerdoisvalores {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int x;
		int y;
		int z;
		
		x = sc.nextInt();
		y = sc.nextInt();
		z = x + y;
		
	    System.out.println("A soma de: " + x + " + " + y + " = " + z);
	    
		
		

	}

}
